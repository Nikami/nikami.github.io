  "scripts": {
    "start_dev": "npm run build_dev && gulp connect",
    "start_prod": "npm run build_prod && gulp connect",
    "build_dev": "gulp dev && npm run watch",
    "build_prod": "gulp prod && npm run aot && npm run rollup && gulp uglify && gulp zip",
    "watch": "concurrently --kill-others  \"npm run tsc:w\" \"gulp watch\"",
    "aot": "ngc -p tsconfig.aot.json",
    "rollup": "rollup -c rollup.config.js",
    "lint": "tslint ./link/**/*.ts -t verbose",
    "tsc:w": "tsc -w --pretty"
  },
